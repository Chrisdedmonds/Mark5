<h1>Certifications#index</h1>
<p>Find me in app/views/certifications/index.html.erb</p>


<table>
	<thead>
		<tr>
			<th>Certification</th>
			<th>Year received</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<% @certifications.each do |certification| %>
			<td><%= certification.name %></td>
			<td><%= certification.year %></td>
		</tr>
		<% end %>
	</tbody>
</table>